<h1 class="feed-title">
  <a href="<%= feed.escape("url") %>"><%= feed.escape("title") %></a>
</h1>

<button class="refresh-entries">Refresh Entries</button>

<ul>
  <% _.each(feed.get('entries').models, function(entry) { %>

    <% var link = "#/feeds/" + entry.get("feed_id") + "/entries/" +  entry.get("id") %>
    <li>
      <a href='<%= link %>'><%= entry.escape('title') %></a>
    </li>

  <% }); %>
</ul>
