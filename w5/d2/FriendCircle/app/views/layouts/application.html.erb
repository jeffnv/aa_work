<!DOCTYPE html>
<html>
<head>
  <title>FriendCircle</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<% flash[:errors].try(:each) do |error| %>
		<%= error %> <br>
	<% end %>

	<%if !!current_user%>
	<h3><%=current_user.email%> <%=button_to "Log Out", session_url, method: :delete%>
	<%end%>
<%= yield %>

</body>
</html>
