<!DOCTYPE html>
<html>
<head>
  <title>RandomThoughts</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<%=session%>
	<% if current_user %>
		<%=current_user.email%>
	<%end%>
	
	<br> <br>
	<%if flash[:errors]%>
		<%flash[:errors].each do |error|%>
			<%=error%>
		<%end%>
	<%end%>
	
<%= yield %>

</body>
</html>
